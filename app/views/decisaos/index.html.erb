<h1>Listing decisaos</h1>

<table>
  <tr>
    <th>Titulo</th>
    <th>Conteudo</th>
    <th>Autor</th>
    <th>Data</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @decisaos.each do |decisao| %>
  <tr>
    <td><%= decisao.titulo %></td>
    <td><%= decisao.conteudo %></td>
    <td><%= decisao.autor %></td>
    <td><%= decisao.data %></td>
    <td><%= link_to 'Show', decisao %></td>
    <td><%= link_to 'Edit', edit_decisao_path(decisao) %></td>
    <td><%= link_to 'Destroy', decisao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Decisao', new_decisao_path %>
